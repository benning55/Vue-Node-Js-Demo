v>\n  </div>\n  <router-view />\n</template>\n\n<script>\n  import {\n    mapActions,\n    mapGetters\n  } from 'vuex'\n  export default {\n    components: {},\n    data() {\n      return {\n        data: null,\n        isActive: false\n      }\n    },\n    computed: {\n      ...mapGetters('auth', {\n        getterLoginStatus: 'getLoginStatus'\n      }),\n      ...mapGetters('home', {\n        getterGetCart: 'getCart' \n      })\n    },\n    methods: {\n      ...mapActions('auth', {\n        actionSignOut: 'signOut'\n      }),\n      atToggle() {\n        this.isActive = !this.isActive\n      },\n      async atSignOut() {\n        await this.actionSignOut()\n      }\n    }\n  }\n</script>\n\n<style scoped>\n  .action {\n    display: block;\n    width: 100%;\n  }\n\n  .action li {\n    line-height: 3rem;\n    border-bottom: 2px solid black;\n  }\n\n  .changedirection {\n    flex-direction: column;\n  }\n\n  .changeSize {\n    width: 40px;\n    margin: 0.5rem 0;\n  }\n\n  .cart-count {\n    font-size: 0.75rem;\n    background-color: red;\n    width: 1rem;\n    text-align: center;\n    color: #fff;\n    z-index: 2;\n    border-radius: 3px;\n    position: absolute;\n    line-height: 1.1rem;\n    right: 0px;\n  }\n</style>"]}]}