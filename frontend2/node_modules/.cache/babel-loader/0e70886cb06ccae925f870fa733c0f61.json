 <ul class=\"sm:flex w-6/12 justify-evenly hidden transition-transform items-center\" :class=\"{'action': isActive}\">\n        <li>\n          <router-link to=\"/\">Home</router-link>\n        </li>\n        <li>\n          <router-link to=\"/about\">About</router-link>\n        </li>\n\n        <li v-if=\"getterLoginStatus !== 'Success'\">\n          <router-link to=\"/signin\">Sign In</router-link>\n        </li>\n        <li v-if=\"getterLoginStatus !== 'Success'\">\n          <router-link to=\"/signup\">Sign Up</router-link>\n        </li>\n        <li v-if=\"getterLoginStatus === 'Success'\" @click=\"atSignOut\">Sign Out</li>\n        <li v-if=\"getterLoginStatus === 'Success'\">\n          <div class=\"w-full relative\" :class=\"{'changeSize': isActive}\">\n            <span class=\"cart-count\">{{ getterGetCart.length }}</span><img src=\"./assets/shopping-cart.svg\" class=\"w-8\" alt=\"\">\n          </div>\n        </li>\n      </ul>\n    </div>\n  </div>\n  <router-view />\n</template>\n\n<script>\n  import {\n    mapActions,\n    mapGetters\n  } from 'vuex'\n  export default {\n    components: {},\n    data() {\n      return {\n        data: null,\n        isActive: false\n      }\n    },\n    computed: {\n      ...mapGetters('auth', {\n        getterLoginStatus: 'getLoginStatus'\n      }),\n      ...mapGetters('home', {\n        getterGetCart: 'getCart' \n      })\n    },\n    methods: {\n      ...mapActions('auth', {\n        actionSignOut: 'signOut'\n      }),\n      atToggle() {\n        this.isActive = !this.isActive\n      },\n      async atSignOut() {\n        await this.actionSignOut()\n      }\n    }\n  }\n</script>\n\n<style scoped>\n  .action {\n    display: block;\n    width: 100%;\n  }\n\n  .action li {\n    line-height: 3rem;\n    border-bottom: 2px solid black;\n  }\n\n  .changedirection {\n    flex-direction: column;\n  }\n\n  .changeSize {\n    width: 40px;\n    margin: 0.5rem 0;\n  }\n\n  .cart-count {\n    font-size: 0.75rem;\n    background-color: red;\n    width: 1rem;\n    text-align: center;\n    color: #fff;\n    z-index: 2;\n    border-radius: 3px;\n    position: absolute;\n    line-height: 1.1rem;\n    right: 0px;\n  }\n</style>"],"sourceRoot":""}]}